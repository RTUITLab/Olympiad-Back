{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "normalize@8.0.1",
      "destination": "wwwroot/css/lib",
      "files": [
        "normalize.min.css"
      ]
    },
    {
      "library": "pdfmake@0.2.4",
      "destination": "wwwroot/js/lib/pdfmake/",
      "files": [
        "pdfmake.min.js",
        "fonts/Roboto/Roboto-Italic.ttf",
        "fonts/Roboto/Roboto-Medium.ttf",
        "fonts/Roboto/Roboto-MediumItalic.ttf",
        "fonts/Roboto/Roboto-Regular.ttf"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "html-to-pdfmake@2.3.10",
      "destination": "wwwroot/js/lib/html-to-pdfmake/",
      "files": [
        "browser.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "roboto-mono-webfont@2.0.986",
      "destination": "wwwroot/js/lib/pdfmake/fonts/RobotoMono/",
      "files": [
        "fonts/RobotoMono-Regular.ttf"
      ]
    },
    {
      "library": "jszip@3.7.1",
      "destination": "wwwroot/js//lib/jszip/",
      "files": [
        "jszip.min.js"
      ]
    }
  ]
}